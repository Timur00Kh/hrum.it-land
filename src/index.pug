doctype html
html(lang="en")
    head
        link(href='index.css', rel='stylesheet')
        meta(name='viewport' content='width=device-width, initial-scale=1')

        title Timur Kh
        meta(property='og:title' content="Timur Kh")
        meta(property='og:description' content='Mostly, web developer')
        meta(property='og:image' content='https://hrum.it/timur.png')
        meta(property='og:type' content='profile')

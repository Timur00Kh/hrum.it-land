                a(target='_blank' href='https://vk.com/timur00kh')
                    img.icon(src='/icons/vk.svg' alt='vk')
                a(target='_blank' href='https://t.me/timur00kh')
                    img.icon.icon-big(src='/icons/tg.svg' alt='telegram')
                a(target='_blank' href='https://instagram.com/timur00kh')
                    img.icon(src='/icons/inst.svg' alt='instagram')
                a(target='_blank' href='https://timurkh.ru')
                    img.icon(src='/icons/globe.svg' alt='my site')

        script(src='https://cdn.jsdelivr.net/npm/typed.js@2.0.12')
